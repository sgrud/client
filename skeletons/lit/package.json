{
  "name": "sgrud-skeleton-lit",
  "amdName": "sgrud.skeleton.lit",
  "version": "0.0.0",
  "source": "./src/index.ts",
  "exports": "./dist/sgrud-skeleton-lit.esm.js",
  "unpkg": "./dist/sgrud-skeleton-lit.umd.js",
  "private": true,
  "scripts": {
    "postinstall": "sgrud runtimify @babel/runtime:helpers",
    "build": "sgrud construct --format modern,umd",
    "postbuild": "sgrud postbuild"
  },
  "amdNames": {
    "@babel/runtime/helpers/inheritsLoose": null,
    "@babel/runtime/helpers/taggedTemplateLiteralLoose": null,
    "lit/decorators.js": "lit"
  },
  "copy": [
    "./src/index.html"
  ],
  "dependencies": {
    "@babel/runtime": "7.16.7",
    "@sgrud/bin": "latest",
    "lit": "2.1.1",
    "tslib": "2.3.1"
  },
  "webDependencies": {
    "@babel/runtime": {
      "exports": {
        "@babel/runtime/helpers/": "./helpers/esm/"
      },
      "unpkg": [
        "./runtimify.js"
      ]
    },
    "@lit/reactive-element": {
      "exports": {
        "@lit/reactive-element": "./reactive-element.js",
        "@lit/reactive-element/": "./"
      }
    },
    "lit": {
      "exports": {
        "lit": "./index.js",
        "lit/": "./"
      }
    },
    "lit-element": {
      "exports": {
        "lit-element/": "./"
      }
    },
    "lit-html": {
      "exports": {
        "lit-html": "./lit-html.js"
      }
    },
    "tslib": {
      "exports": {
        "tslib": "./tslib.es6.js"
      },
      "unpkg": [
        "./tslib.js"
      ]
    }
  }
}
