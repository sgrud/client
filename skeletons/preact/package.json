{
  "name": "sgrud-skeleton-preact",
  "amdName": "sgrud.skeleton.preact",
  "version": "0.0.0",
  "source": "./src/index.ts",
  "exports": "./dist/sgrud-skeleton-preact.esm.js",
  "unpkg": "./dist/sgrud-skeleton-preact.umd.js",
  "private": true,
  "scripts": {
    "build": "sgrud construct --format modern,umd",
    "postbuild": "sgrud postbuild && cp ./src/index.html ./dist",
    "prepare": "sgrud runtimify @babel/runtime:helpers"
  },
  "amdNames": {
    "@babel/runtime/helpers/inheritsLoose": null,
    "preact/jsx-runtime": "jsxRuntime"
  },
  "dependencies": {
    "@babel/runtime": "7.16.3",
    "@sgrud/bin": "../../dist/bin",
    "preact": "10.6.1",
    "preact-custom-element": "4.2.1",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@types/css-modules": "1.0.2",
    "@types/preact-custom-element": "4.0.1",
    "node-sass": "6.0.1"
  },
  "webDependencies": {
    "@babel/runtime": {
      "exports": {
        "@babel/runtime/helpers/": "./helpers/esm/"
      },
      "unpkg": [
        "./runtimify.js"
      ]
    },
    "preact": {
      "exports": {
        "preact": "./dist/preact.module.js",
        "preact/jsx-runtime": "./jsx-runtime/dist/jsxRuntime.modern.js"
      },
      "unpkg": [
        "./dist/preact.umd.js",
        "./jsx-runtime/dist/jsxRuntime.umd.js"
      ]
    },
    "preact-custom-element": {
      "exports": {
        "preact-custom-element": "./dist/preact-custom-element.esm.js"
      },
      "unpkg": [
        "./dist/preact-custom-element.umd.js"
      ]
    },
    "tslib": {
      "exports": {
        "tslib": "./tslib.es6.js"
      },
      "unpkg": [
        "./tslib.js"
      ]
    }
  }
}
