{
  "name": "sgrud-example-static",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npm pack ../../dist/core ../../skeletons/*/dist",
    "postinstall": "npm install --no-save *.tgz && rm *.tgz",
    "prepare": "sgrud runtimify @babel/runtime:helpers lit:!static",
    "start": "sgrud universal -H 0.0.0.0 src/index.html"
  },
  "dependencies": {
    "core-js-bundle": "3.19.1",
    "es-module-shims": "1.3.2"
  },
  "optionalDependencies": {
    "@sgrud/core": "*",
    "sgrud-skeleton-angular": "*",
    "sgrud-skeleton-lit": "*",
    "sgrud-skeleton-preact": "*",
    "sgrud-skeleton-vue": "*"
  },
  "sgrudDependencies": {
    "sgrud-skeleton-angular": "*",
    "sgrud-skeleton-lit": "*",
    "sgrud-skeleton-preact": "*",
    "sgrud-skeleton-vue": "*"
  }
}
