{
  "name": "sgrud-example-static",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npm-run-all preinstall:*",
    "preinstall:bin": "npm pack ../../dist/bin",
    "preinstall:core": "npm pack ../../dist/core",
    "preinstall:skeletons": "npm pack ../../skeletons/*/dist",
    "postinstall": "npm install --no-audit --no-save *.tgz && rm *.tgz",
    "prepare": "sgrud runtimify @babel/runtime:helpers lit:!static",
    "start": "sgrud universal -H 0.0.0.0 src/index.html"
  },
  "dependencies": {
    "core-js-bundle": "3.19.2",
    "es-module-shims": "1.3.4",
    "npm-run-all": "4.1.5"
  },
  "optionalDependencies": {
    "@sgrud/bin": "*",
    "@sgrud/core": "*",
    "sgrud-skeleton-angular": "*",
    "sgrud-skeleton-lit": "*",
    "sgrud-skeleton-preact": "*",
    "sgrud-skeleton-vue": "*"
  },
  "sgrudDependencies": {
    "sgrud-skeleton-angular": "*",
    "sgrud-skeleton-lit": "*",
    "sgrud-skeleton-preact": "*",
    "sgrud-skeleton-vue": "*"
  }
}
